page {
  width: 100%;
  margin: 0;
  /* background: #F2F2F2; */
}

.left,
.right {
  display: inline-block;
  vertical-align: top;
  width: 49%;
}

.right {
  float: right;
  margin-left: 1%;
}

.left {
  float: left;
  margin-right: 1%;
}

.item {
  display: inline-block;
  background: rgba(255, 255, 255, 0.705);
  margin-bottom: 10rpx;
  border-radius: 10px;
  border: 1px #eef0ed solid;
  width: 100%;
}

.pic {
  width: 100%;
  border-radius: 10rpx 15rpx 0 0;
}

.level {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

#star {
  display: flex;
  align-items: center;
  margin-right: 2rpx;
  height: 40rpx;
  width: 40rpx;
}

.bottom {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.title-wrap {
  padding: 5rpx 20rpx 0rpx 20rpx;
  font-size: 30rpx;
  font-weight: 600;
  color: #333;
}

.title {
  margin-top: 5rpx;
  margin-left: 10rpx;
  font-size: 14px;
  font-family: 'PingFang SC-Medium';
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.face {
  width: 70rpx;
  height: 70rpx;
  border-radius: 100%;
  float: left;
  margin-left: 10rpx;
  margin-right: 20rpx;
}

.name {
  overflow: hidden;
  width: 100rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: nowrap;
  font-size: 13px;
  font-family: 'PingFang SC-Medium';
  color: rgb(58, 57, 57);
  display: block;
}

.face-name {
  display: flex;
  align-items: center;
  width: 100%;
  height: 100%;
  margin: 20rpx 0rpx;
  padding: 10rpx 0 10rpx 10rpx;
}

.interact {
  display: flex;
  align-items: center;
}

.good,
.collection {
  display: flex;
  align-items: center;
}

.good_icon,
.goods_icon,
.collection_icon,
.collections_icon {
  width: 35rpx;
  height: 35rpx;
  margin-right: 5rpx;
  margin-right: 20rpx
}

.goods_icon {
  width: 37rpx;
  height: 37rpx;
}

.collections_icon {
  width: 43rpx;
  height: 43rpx;
}