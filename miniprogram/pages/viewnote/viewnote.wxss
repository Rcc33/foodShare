/* miniprogram/pages/viewnote/viewnote.wxss */
.host {
  width: 100%;
  margin: 2px;
  line-height: 40px;
  display: flex;
}

.collections_icon{
  width: 53rpx;
  height: 53rpx;
}

.collection_icon{
  width: 47rpx;
  height: 47rpx;
  margin-right: 2px;
}

.profile {
  width: 40px;
  height: 40px;
  margin-left: 8px;
  align-items: center;
  display: flex;
  justify-content: center;
}

.avatar {
  width: 40px;
  height: 40px;
  border-radius: 50%;
}

.name {
  margin-left: 20px;
}

.photo {
  width: 100%;
}

#photo {
  align-items: center;
  display: flex;
  justify-content: center;
}

.title {
  margin: 15px;
  font-size: 38rpx;
  margin-top: 20px;
  font-weight: 550;
  font-family: 'Times New Roman', 'PingFang SC-Medium';
}

.detail {
  margin: 15px;
  font-family: 'Times New Roman', 'PingFang SC-Medium';
  letter-spacing: 3rpx;
  line-height: 30px;
  font-size: 32rpx;
}

.timelocation {
  display: flex;
  height: 20px;
  line-height: 20px;
  font-size: 30rpx;
  color: rgb(172, 172, 172);
  margin-right: 20px;
  margin-left: 15px;
  justify-content: space-between;
}

.location {
  display: flex;
  width: 45%;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
}

.locate_image{
  width: 12px;
}

.locate_text{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  margin-left: 5rpx;
}

.locatepic {
  height: 12px;
  width: 12px;
}

.line {
  margin: 15px;
  border-top: 1px #eeeaea solid;
}

.grade {
  margin: 15px;
  font-style: inherit;
  color: darkgrey;
  align-items: center;
  display: flex;
}

.foodfeel {
  margin-right: 10px;
}

#star {
  margin: 2px;
  height: 60rpx;
  width: 60rpx;
  display: inline-block;
}

.handel {
  margin: 15px;
  height:30px;
  width: 93%;
  display: flex;
  justify-content: space-between;
}

#comment_block_input {
  width: 100px;
  background-color: rgba(219, 217, 217, 0.295);
  height: 30px;
  padding-left: 8px;
  margin-right: 20px;
  border-radius: 35rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.like_comment_collect{
  display: flex;
  flex-direction: row;
}
#pencil{
  width: 30px;
  height: 20px;
  margin-right: 5px;
}
#comment_input{
  color:rgb(107, 103, 103);
}
.handel_style {
  width: 55px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
#like_image{
  width: 20px;
  height: 20px;
  margin-right: 2px;
}
#review_image{
  width: 25px;
  height: 25px;
  margin-right: 2px;
}

.comment {
  height: 20px;
  font-weight: 30px;
  background-color: gold;
}

@font-face {
  font-family: 'iconfont';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
    url('data:font/woff;charset=utf-8;base64,') format('woff'),
    url('iconfont.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fanhui:before {
  content: "\e604";
}